<tryton>
    <data>
        <record model="ir.ui.view" id="address_view_form">
            <field name="model">party.address</field>
            <field name="inherit" ref="party.address_view_form"/>
            <field name="name">address_form</field>
        </record>

        <record model="ir.ui.view" id="address_view_form_simple2">
            <field name="model">party.address</field>
            <field name="type">form</field>
            <field name="name">address_form_simple</field>
        </record>
    </data>

    <data depends="stock">
        <record model="ir.ui.view" id="address_view_form_simple2_stock">
            <field name="model">party.address</field>
            <field name="inherit" ref="account_invoice_facturae.address_view_form_simple2"/>
            <field name="name">address_form_simple_stock</field>
        </record>
    </data>

    <data depends="sale_invoice_grouping">
        <record model="ir.ui.view" id="address_sale_grouping_view_form">
            <field name="model">party.address</field>
            <field name="inherit" ref="party.address_view_form"/>
            <field name="name">address_sale_grouping_form</field>
        </record>
    </data>

    <data depends="party_zip">
        <record model="ir.ui.view" id="address_view_form_simple_party_zip">
            <field name="model">party.address</field>
            <field name="inherit" ref="account_invoice_facturae.address_view_form_simple2"/>
            <field name="name">address_form_simple_party_zip</field>
        </record>
    </data>
</tryton>
