<form col="6">
    <notebook>
        <page id="general" string="General">
            <label name="party"/>
            <field name="party" colspan="5"/>
            <label name="party_name"/>
            <field name="party_name"/>
            <group colspan="2" col="-1" id="checkboxes">
                <label name="active"/>
                <field name="active" xexpand="0" width="25"/>
                <label name="invoice"/>
                <field name="invoice" xexpand="0" width="25"/>
                <label name="sequence"/>
                <field name="sequence"/>
            </group>
            <newline/>
            <label name="name"/>
            <field name="name"/>
            <newline/>
            <label name="street" yalign="0"/>
            <field name="street" colspan="3" height="80"/>
            <newline/>
            <label name="postal_code"/>
            <field name="postal_code"/>
            <label name="city"/>
            <field name="city"/>
            <newline/>
            <label name="country"/>
            <field name="country"/>
            <label name="subdivision"/>
            <field name="subdivision"/>
        </page>
        <page id="einvoice" string="eInvoice">
            <label name="facturae_person_type"/>
            <field name="facturae_person_type"/>
            <label name="facturae_residence_type"/>
            <field name="facturae_residence_type"/>
            <newline/>
            <label name="oficina_contable"/>
            <field name="oficina_contable"/>
            <label name="organo_gestor"/>
            <field name="organo_gestor"/>
            <label name="unidad_tramitadora"/>
            <field name="unidad_tramitadora"/>
            <label name="organo_proponente"/>
            <field name="organo_proponente"/>
        </page>
    </notebook>
</form>
